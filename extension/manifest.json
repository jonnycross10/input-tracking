{
    "name": "extension",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "tracking.js",
      "type": "module"
    },
    "permissions": ["activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html"
      }
  }